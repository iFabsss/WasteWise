<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/wastewiseicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/wastewiseicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.WasteWise"
        tools:targetApi="34">
        <activity
            android:name=".RewardsActivity"
            android:exported="false" />
        <activity
            android:name=".CompletedQuestsActivity"
            android:exported="false" />
        <activity
            android:name=".AvailableQuestsActivity"
            android:exported="false" />
        <activity
            android:name=".QuestMenuActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".StartupActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>